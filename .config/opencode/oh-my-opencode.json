{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "disabled_mcps": [],
  "agents": {
    "build": {
      "model": "anthropic/claude-opus-4-5"
    },
    "plan": {
      "model": "anthropic/claude-opus-4-5"
    },
    "sisyphus": {
      "model": "anthropic/claude-opus-4-5"
    },
    "sisyphus-junior": {
      "model": "anthropic/claude-opus-4-5"
    },
    "prometheus": {
      "model": "anthropic/claude-opus-4-5"
    },
    "metis": {
      "model": "anthropic/claude-opus-4-5"
    },
    "momus": {
      "model": "anthropic/claude-opus-4-5"
    },
    "oracle": {
      "model": "anthropic/claude-opus-4-5",
      "thinking": {
        "type": "enabled",
        "budgetTokens": 200000
      }
    },
    "librarian": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "explore": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "general": {
      "model": "anthropic/claude-opus-4-5"
    },
    "document-writer": {
      "model": "google/antigravity-gemini-3-pro",
      "description": "Norwegian technical blog writer for AI and agentic development patterns",
      "prompt_append": "You are writing for a Norwegian technical blog about AI and agentic development patterns. Follow AGENTS.md exactly. Always write in Norwegian (bokmal) but keep technical terms in English (e.g., 'embedding', 'state machine', 'prompt engineering')."
    },
    "multimodal-looker": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "hephaestus": {
      "model": "anthropic/claude-opus-4-5",
      "variant": "max"
    },
    "atlas": {
      "model": "anthropic/claude-opus-4-5"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "google/antigravity-gemini-3-pro"
    },
    "ultrabrain": {
      "model": "anthropic/claude-opus-4-5",
      "variant": "max",
      "thinking": {
        "type": "enabled",
        "budgetTokens": 200000
      }
    },
    "deep": {
      "model": "anthropic/claude-opus-4-5",
      "variant": "max",
      "thinking": {
        "type": "enabled",
        "budgetTokens": 200000
      }
    },
    "artistry": {
      "model": "google/antigravity-gemini-3-pro",
      "variant": "high"
    },
    "quick": {
      "model": "google/antigravity-gemini-3-flash"
    },
    "unspecified-low": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "unspecified-high": {
      "model": "anthropic/claude-opus-4-5",
      "variant": "max"
    },
    "writing": {
      "model": "google/antigravity-gemini-3-flash"
    }
  },
  "background_task": {
    "defaultConcurrency": 5,
    "providerConcurrency": {
      "anthropic": 3,
      "google": 8
    }
  }
}
